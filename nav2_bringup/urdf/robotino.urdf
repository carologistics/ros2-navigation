  </joint>

  <!-- fingers_home Link and Joint -->
  <link name="fingers_home"/>
  <joint name="joint_gripper_home_to_fingers_home" type="fixed">
    <parent link="gripper_home"/>
    <child link="fingers_home"/>
    <origin xyz="0.2 0 -0.04" rpy="0 0 0"/>
  </joint>

  <!-- shelf_record Link and Joint -->
  <link name="shelf_record"/>
  <joint name="joint_gripper_to_shelf_record" type="fixed">
    <parent link="gripper"/>
    <child link="shelf_record"/>
    <origin xyz="0.2 0.06 -0.04" rpy="-1.57 0 -1.57"/>
  </joint>

</robot>