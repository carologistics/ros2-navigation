<robot name="robot">

  <!-- Base Link -->
  <link name="base_link"/>

  <!-- front_laser Link and Joint -->
  <link name="front_laser"/>
  <joint name="joint_base_link_to_front_laser" type="fixed">
    <parent link="base_link"/>
    <child link="front_laser"/>
    <origin xyz="0.094 0 0.2390" rpy="3.1415 0 0"/>
  </joint>

  <!-- base_laser Link and Joint -->
  <link name="base_laser"/>
  <joint name="joint_base_link_to_base_laser" type="fixed">
    <parent link="base_link"/>
    <child link="base_laser"/>
    <origin xyz="0.094 0 0.2390" rpy="0 0 0"/>
  </joint>

  <!-- back_laser Link and Joint -->
  <link name="back_laser"/>
  <joint name="joint_base_link_to_back_laser" type="fixed">
    <parent link="base_link"/>
    <child link="back_laser"/>
    <origin xyz="-0.11 0 0.32" rpy="0 2.94 0"/>
  </joint>

  <!-- cam_tag Link and Joint -->
  <link name="cam_tag"/>
  <joint name="joint_base_link_to_cam_tag" type="fixed">
    <parent link="base_link"/>
    <child link="cam_tag"/>
    <origin xyz="0.0546 0 0.4675" rpy="-1.57079632679 -1.57079632679 -1.57"/>
  </joint>

  <!-- plate_top Link and Joint -->
  <link name="plate_top"/>
  <joint name="joint_base_link_to_plate_top" type="fixed">
    <parent link="base_link"/>
    <child link="plate_top"/>
    <origin xyz="0 0 0.768" rpy="0 0 0"/>
  </joint>

  <!-- gripper_home Link and Joint -->
  <link name="gripper_home"/>
  <joint name="joint_plate_top_to_gripper_home" type="fixed">
    <parent link="plate_top"/>
    <child link="gripper_home"/>
    <origin xyz="0.0312 0 0.1808" rpy="0 0 0"/>
  </joint>

  <!-- end_effector_home Link and Joint -->
  <link name="end_effector_home"/>
  <joint name="joint_gripper_home_to_end_effector_home" type="fixed">
    <parent link="gripper_home"/>
