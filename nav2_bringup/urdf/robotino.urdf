  </joint>

  <!-- gripper_fingers_record Link and Joint -->
  <link name="gripper_fingers_record"/>
  <joint name="joint_gripper_to_gripper_fingers_record" type="fixed">
    <parent link="gripper"/>
    <child link="gripper_fingers_record"/>
