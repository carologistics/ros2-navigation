  </joint>

  <!-- gripper_fingers Link and Joint -->
  <link name="gripper_fingers"/>
  <joint name="joint_gripper_to_gripper_fingers" type="fixed">
    <parent link="gripper"/>
    <child link="gripper_fingers"/>
