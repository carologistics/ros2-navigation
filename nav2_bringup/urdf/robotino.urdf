  </joint>

  <!-- gripper_y_origin Link and Joint -->
  <link name="gripper_y_origin"/>
  <joint name="joint_gripper_x_dyn_to_gripper_y_origin" type="fixed">
    <parent link="gripper_x_origin"/>
    <child link="gripper_y_origin"/>
    <origin xyz="-0.013 0 0.034" rpy="0 0 0"/>
  </joint>

  <!-- gripper_z_origin Link and Joint -->
  <link name="gripper_z_origin"/>
  <joint name="joint_gripper_y_dyn_to_gripper_z_origin" type="fixed">
    <parent link="gripper_y_origin"/>
    <child link="gripper_z_origin"/>
